{
  "env": {
    "browser": true
  },
  "extends": "eslint-config-airbnb",
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true,
    "destructuring": true,
    "classes": true,
    "forOf": true,
    "blockBindings": true,
    "arrowFunctions": true
  },
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
      "error",
      "single"
    ],
    "semi": 0,
    "no-console": 0,
    "no-debugger": 0,
    "no-unused-vars": 0,
    "no-multi-spaces": [1, {
      "exceptions": {
        "Property": true,
        "ImportDeclaration": true,
        "VariableDeclarator": true
      }
    }],
    "react/jsx-filename-extension": 0,
    "space-before-function-paren": ["error", "always"],
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "import/extensions": ["error", {
      "js": "never"
    }],
    "react/prefer-stateless-function": 0,
    "comma-dangle": ["error", {
        "arrays": "never",
        "objects": "never",
        "imports": "never",
        "exports": "never",
        "functions": "ignore"
    }]
  }
}
